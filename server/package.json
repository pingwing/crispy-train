{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch src --ext ts,graphql --exec ts-node src/index.ts",
    "build": "tsc -p tsconfig.json && node scripts/copy-schema.mjs",
    "start": "node dist/index.js",
    "mikro-orm": "mikro-orm",
    "migration:up": "mikro-orm migration:up",
    "migration:down": "mikro-orm migration:down",
    "migration:create": "mikro-orm migration:create",
    "seed": "npm run build && node dist/db/seed/runSeed.js",
    "test": "npm run build && node --test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@apollo/server": "^5.2.0",
    "@as-integrations/express5": "^1.1.2",
    "@graphql-tools/schema": "^10.0.30",
    "@mikro-orm/core": "^6.6.2",
    "@mikro-orm/migrations": "^6.6.2",
    "@mikro-orm/postgresql": "^6.6.2",
    "@mikro-orm/reflection": "^6.6.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "graphql": "^16.12.0",
    "reflect-metadata": "^0.2.2",
    "uuid": "^13.0.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^6.6.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
